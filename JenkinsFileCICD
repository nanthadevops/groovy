#!/usr/bin/env groovy
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('DeployProd') {
            steps {
                echo 'Deploying to production....'
                def sqs ='Hello'
                def dqs = "Nantha!"
                
                def username = "rb"
                echo '${sqs} ${dqs}'
                echo "I said , Hello Mr.${username}"
            }
        }
    }
}
