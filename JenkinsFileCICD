#!/usr/bin/env groovy
pipeline {
    agent any
    environment { 
        CC = 'Nantha'
    }
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage('DeployProd') {
            steps {
                echo 'Deploying to production....'
                sh 'printenv'
            }
        }
    }
}
